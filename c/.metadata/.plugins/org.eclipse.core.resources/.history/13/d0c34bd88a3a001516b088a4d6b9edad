#include <stdio.h>
#include <stdlib.h>

#define MAX 350
int check( int n, int r);
int isReachable( int n, int x1, int y1, int x2, int y2);
int maze[MAX][MAX], rat[MAX][MAX];
int main(void) {
	int r, i, j, n;
	char ch;

	scanf("%d",&n);
	for(i=0; i< n; i++){
		for(i=0; i<n; i++){
			scanf("%c",&ch);
			if(ch == 'X' || ch == 'x'){
				maze[i][j] = 0;
			}
			else if(ch == 'O' || ch == 'o'){
				maze[i][j] = 1;
			}
		}
	}
	scanf("%d",&r);
	for(i=0; i<r; i++){
		scanf("%d %d",&rat[i][0], &rat[i][1]);
	}
	int flag = 0 ;
	flag = check(n,r);
	if(flag){
		printf("YES");
	}else{
		printf("NO");
	}
	return EXIT_SUCCESS;
}

int check(int n, int r){
	int reached[r][r];
	int i =0,j =0;
	int flag = 0;
	for(i = 0 ;i<r; i++){
		for(j=i+1; j<r; j++){
			flag = isReachable( n-1, rat[i][0], rat[i][1], rat[j][0], rat[j][1]);
			if(flag){
				reached[i][j] = 1;
			}
			else {
				reached[i][j] = 0;
			}
		}
	}
	for(i=0; i<r; i++){
		for(j=0; j<r; j++){
			if(reached[i][j] == 0){
				return 0;
			}
		}
	}
	return 1;
}

int isReachable( int n, int x1, int y1, int x2, int y2){
	if(rat1[0] == rat2[0] && rat1[1] == rat2[1]){
		return 1;
	}
	if(rat1[1]<n && maze[rat1[0]][rat1[1]+1] == 1){
		rat1[1]++;
		return isReachable( n, rat1, rat2);
	}
	if(rat1[0]<n && maze[rat1[0]+1][rat1[1]] == 1){
		rat1[0]++;
		return isReachable( n, rat1, rat2);
	}
	if(rat1[1]>n && maze[rat1[0]][rat1[1]-1] == 1){
		rat1[1]--;
		return isReachable( n, rat1, rat2);
	}
	if(rat1[1]>n && maze[rat1[0]-1][rat1[1]+1] == 1){
		rat1[0]--;
		return isReachable( n, rat1, rat2);
	}
	return 0;
}
