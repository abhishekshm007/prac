 */
#include<stdio.h>

int main(){
	int t;
	float p,r;
	scanf("%f",&p);
	scanf("%f",&r);
	scanf("%d",&t);
	float te;
	while(t){
		te = (((p*r)/(100.0*12.0)));
		printf("%f\n",te);
		p = p+te;
		t--;
	}
	printf("%f",p);
	return 0;
}

