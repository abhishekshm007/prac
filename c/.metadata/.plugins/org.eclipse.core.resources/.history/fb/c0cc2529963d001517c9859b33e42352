#include<stdio.h>

int main(){
	int t;
	double p,r;
	scanf("%lf",&p);
	scanf("%lf",&r);
	scanf("%d",&t);
	double te;
	while(t){
		te = (((p*r)/(100.0)));
		printf("%lf\n",te);
		p = p+te;
		t--;
	}
	printf("%f",p);
	return 0;
}

