#include<stdio.h>

int main(){
	int t;
	double p,r;

	scanf("%lf",&p);
	scanf("%lf",&r);
	scanf("%d",&t);
	double f=p;
	double te;
	while(t){
		te = (((p*r)/(100.0*12.0)));
		printf("%lf\n",te);
		p = p+te+f;
		t--;
	}
	double x = p-f;
	int y = p-f;
	float c = x-p;
	printf("%.0f",x);
	if(c>=.5){
		y++;
	}
//	printf("%i",y);
	return 0;
}

